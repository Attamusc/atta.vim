{
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": false,
  "eslint.quiet": false,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.disableSuccessMessage": true,
  "prettier.requireConfig": true,
  "tsserver.enableJavascript": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "rust",
    "json"
  ],
  "coc.preferences.formatOnType": false
}
